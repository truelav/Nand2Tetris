/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notSel)
    And(a=a, b=notSel, out=and1Out)
    And(a=b, b=notSel, out=and2Out)
    Or(a=and1Out, b=and2Out, out=out)
}